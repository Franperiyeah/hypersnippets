{
    "name": "Hypersnippets",
    "displayName": "Hyper snippets",
    "description": "A complete and constantly updated snippet for various languages.",
    "version": "1.0.2",
    "publisher": "FranRuizSantaclara",
    "repository": {
        "type": "git",
        "url": "https://github.com/Franperiyeah/hypersnippets"
    },
    "icon": "images/logo.png",
    "keywords": [
        "snippets",
        "best snippets",
        "hyper",
        "hyper snippets",
        "shortcuts",
        "hypersnippets"
    ],
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "React",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
